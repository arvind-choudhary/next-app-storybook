    "clsx": "^2.0.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.4",
    "eslint-plugin-prettier": "^5.0.0",
    "i18next": "^23.4.6",
    "i18next-browser-languagedetector": "^7.1.0",
