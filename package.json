    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2",
    "storybook": "^7.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn lint --fix",
      "prettier --write --ignore-unknown",
